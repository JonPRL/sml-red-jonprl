// Recursion is built into the tactic language.
Tac repeat(T : tac) = [
  rec R. {
    T; R
  }
].

Tac complete(T : tac) = [
  T; fail
].

Def id(M : exp) : exp = [
  M
].

Thm example : [~{exp}(Ax; id(id(Ax)))] by [
  csym; ceval
].

Thm extractTest : [~{exp}(extract(example); Ax)] by [
  ceval
].
