Thm boolTest : [ (x : bool) -> bool true ] by [
  lam x {
    if x then { 'tt } else { 'ff }
  };
  auto
].

Print boolTest.

Thm pathTest : [ paths({x}. S1; base; base) true ] by [
  <x> {
    'loop{x}
  };
  auto
].

Print pathTest.

Thm funElimTest : [ (f : (x : bool) -> bool) -> bool true ] by [
  lam f {
    let x = f { 'tt } in {
      hyp{x}
    }
  };

  auto
].

Print funElimTest.
