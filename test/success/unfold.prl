Def Times(#A; #B) = [
  (* #A #B)
].

Tac Proj1{z:hyp} = [
  decompose <proj1> = z.
  hyp proj1
].

Tac Proj2{z:hyp} = [
  decompose <proj2> = z.
  hyp proj2
].

Thm Times/Proj(#A) : [
  a/type : #A type 
  >> (-> (Times bool #A) #A)
] by [
  lam x. 
  unfold Times;
  {Proj2 x}
].

Extract Times/Proj.