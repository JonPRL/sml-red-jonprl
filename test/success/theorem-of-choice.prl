Thm Choice(#i:lvl) : [
  (->
   [a b : (U #i)]
   [r : (-> a b (U #i))]
   [f : (-> [x : a] (* [y : b] ($ r x y)))]
   (*
    [f : (-> a b)]
    (-> [x : a] ($ r x ($ f x)))))
] by [
  lam a b r f =>
    {lam x => let {y,_} = f [`x] in `y,
     lam x => let {_,z} = f [`x] in `z};
    inversion; reduce; auto
].

Print Choice.