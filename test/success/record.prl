Thm RecordTest0 : [
  tuple in record
] by [
  auto;
].

Thm RecordTest1 : [
  (tuple [a tt]) in (record [a : bool])
] by [
  auto;
].

Thm RecordTest2 : [
  (tuple [a tt] [b tuple]) in (record [b : record] [a : bool])
] by [
  auto;
].

Thm RecordTest3 : [
  (tuple [a tt] [b ff]) in (record [b a : bool])
] by [
  auto;
].

Thm RecordTest4 : [
  (! a (tuple [a tt] [b ff])) = tt in bool
] by [
  auto;
].

Thm RecordTest5(#p) : [
  #p in record
  >>
  #p = tuple in record
] by [
  auto;
].

