Thm Connection{l:lvl} : [
  (->
   [ty : (U l kan)]
   [a b : ty]
   [p : (path {_} ty a b)]
   (path {i} (path {_} ty a (@ p i)) (abs {_} a) p))
] by [
  lam ty, a, b, p.
    <i j>
      `(hcom{0~>1} ,ty ,a
        [i=0 {k}
          (hcom{1~>j} ,ty
           (hcom{1~>0} ,ty (@ ,p k)
            [k=0 {_} ,a]
            [k=1 {l} (@ ,p l)])
           [k=0 {_} ,a]
           [k=1 {_} ,a])]
        [i=1 {k}
         (hcom{1~>j} ,ty (@ ,p k)
          [k=0 {_} ,a]
          [k=1 {l} (@ ,p l)])]
        [j=0 {k}
          (hcom{1~>i} ,ty
           (hcom{1~>0} ,ty (@ ,p k)
            [k=0 {_} ,a]
            [k=1 {l} (@ ,p l)])
           [k=0 {_} ,a]
           [k=1 {_} ,a])]
        [j=1 {k}
         (hcom{1~>i} ,ty (@ ,p k)
          [k=0 {_} ,a]
          [k=1 {l} (@ ,p l)])])
].

Print Connection.
Extract Connection.
