Thm Connection(#A) : [
  a/type : #A type >>
  (-> [a #A] [b #A]
   [p (paths {_} #A a b)]
   (paths {i} (paths {_} #A a (@ p i)) (abs {_} a) p))
] by [
  {
  lam a. lam b. lam p.
    <i> <j> 
      '(hcom{0~>1} #A ,a
        [i=0 -> {k}
          (hcom{1~>j} #A (hcom{1~>0} #A (@ ,p k) [k=0 -> {_} ,a] [k=1 -> {l} (@ ,p l)])
           [k=0 -> {_} ,a]
           [k=1 -> {_} ,a])]
        [i=1 -> {k} (hcom{1~>j} #A (@ ,p k) [k=0 -> {_} ,a] [k=1 -> {l} (@ ,p l)])]
        [j=0 -> {k}
          (hcom{1~>i} #A (hcom{1~>0} #A (@ ,p k) [k=0 -> {_} ,a] [k=1 -> {l} (@ ,p l)])
           [k=0 -> {_} ,a]
           [k=1 -> {_} ,a])]
        [j=1 -> {k} (hcom{1~>i} #A (@ ,p k) [k=0 -> {_} ,a] [k=1 -> {l} (@ ,p l)])])
  };
  auto
].

Extract Connection.
