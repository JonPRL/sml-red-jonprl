// New hcom format! (Parentheses were intentionally added.)
Thm Hcom(#A) : [
  a/type : #A type
  >>
    (a : #A)
      -> (b : #A)
      -> paths({_}.#A; a; b)
      -> paths({_}.#A; a; b)
] by [
  { lam a. lam b. lam p.
    <i> 'hcom{0 ~> 1}(#A; ,p @ i; i = 1 -> {_}. ,b; (i = 0) -> {_}. ,a)
  };
  auto
].

// New fhcom format! (Parentheses were intentionally added.)
Thm FHcom() : [
     paths({_}.bool; tt; tt)
  -> paths({_}.bool; tt; tt)
] by [
  { lam p.
    <i> 'fhcom{0 ~> 1}(,p @ i; (((i = 1)))-> {_}. tt; (i = 0)-> {_}. tt)
  };
  auto
].
