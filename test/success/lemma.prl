Thm MyLemma : [ x : bool true >> bool true ] by [
  {if x then 'ff else 'tt};
  auto
].

Thm MyCorollary : [ bool -> bool true ] by [
  { lam z. lemma MyLemma{z}
  }; auto
].

Extract MyCorollary.

Thm MyCorollary/Test : [ MyCorollary = lam([z]. if([_]. bool; z; ff; tt)) in bool -> bool ] by [
  auto
].