Thm FHcom/trans3 : [
  (-> [a bool] [b bool] [c bool] [d bool]
   (paths {_} bool a b)
   (paths {_} bool a c)
   (paths {_} bool b d)
   (paths {_} bool c d))
] by [
  { <a> lam b. lam c. lam d.
    lam pab. lam pac. lam pbd.
    <i> '(fcom{0~>1} (@ ,pab i) [i=0 -> {j} (@ ,pac j)] [i=1 -> {j} (@ ,pbd j)])
  };
  auto
].

Thm FHcom/trans2 : [
  (-> [a bool] [b bool] [c bool]
   (paths {_} bool a b)
   (paths {_} bool b c)
   (paths {_} bool a c))
] by [
  { lam a. lam b. lam c. lam pab. lam pbc.
    <i> '(fcom{0~>1} (@ ,pab i) [i=0 -> {_} ,a] [i=1 -> {j} (@ ,pbc j)])
  };
  auto
].

Thm FHcom/symm : [
  (-> [a bool] [b bool]
   (paths {_} bool a b)
   (paths {_} bool b a))
] by [
  { lam a. lam b. lam pab.
    <i> '(fcom{0~>1} ,a [i=0 -> {j} (@,pab j)] [i=1 -> {_} ,a])
  };
  auto
].

Thm Tube : [
  x : bool
  >> (fcom{0~>1} x [1=1 -> {_} x] [0=0 -> {_} x]) = x in bool
] by [
  auto
].

Thm TrueByEvaluation : [
  (fcom{0 ~> 0} tt) in bool
] by [
  auto
].
