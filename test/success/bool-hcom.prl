Thm BasicHcom : [
  (a : bool) -> (b : bool) -> (c : bool) -> (d : bool) ->
  paths({_}.bool; a; b) ->
  paths({_}.bool; a; c) ->
  paths({_}.bool; b; d) ->
  paths({_}.bool; c; d) true
] by [
  { lam a. lam b. lam c. lam d.
    lam pab. lam pac. lam pbd.
    <i> 'hcom{i; 0 ~> 1}(bool; ,pab @ i; {j}. ,pac @ j; {j}. ,pbd @ j)
  }; auto
].
