Thm Line/Test0 : [
  (->
   [a : (U 0 kan)]
   [l : (-> dim a)]
   (= a (coe 0~>1 [_] a (@ l 0)) (@ (coe 0~>1 [_] (-> dim a) l) 0)))
] by [
  lam a l => `ax
].

Thm Line/Test1 : [
  (->
   [ty : (U 0 kan)]
   [p : (line [_] ty)]
   (path [_] ty (@ p 0) (@ p 1)))
] by [
  lam ty p => abs x => `(@ p x)
].
